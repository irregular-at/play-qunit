<?xml version="1.0" encoding="UTF-8" ?>
<testsuite failures="${result.summary.failed}" time="${result.summary.runtime}" errors="0" skipped="0" tests="${result.summary.total}" name="${result.test}">
#{list items:result.tests, as:'test'}
  <testcase name="${test.name.escapeXml()}">
    #{if !test.result}
        <failure type="" message="${test.errorMessage().escapeXml()}">
        <![CDATA[
          ${test.source}
        ]]>
        </failure>
    #{/if}
  </testcase>
#{/list}
</testsuite>